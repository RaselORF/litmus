apiVersion: storage.k8s.io/v1
kind: StorageClass
metadata:
  name: "{{ sc_name }}"
  annotations:
    openebs.io/cas-type: cstor
    cas.openebs.io/config: |
      - name: StoragePoolClaim
        value: "{{ pool_name }}"
      - name: ReplicaCount
        value: "{{ replica_count }}"
      - name: TargetNodeSelector
        value: |-
            lkey: lvalue
provisioner: openebs.io/provisioner-iscsi
